<script language='vbscript'>
On Error Resume Next
Set objWMIService = GetObject("winmgmts:\\.\root\cimv2")
Set colOperatingSystems = objWMIService.ExecQuery("Select * from Win32_OperatingSystem")
For Each objOperatingSystem in colOperatingSystems
	OSversion = replace(objOperatingSystem.Version,".","")
Next
VersionOS.value = OSversion
'Dim Wth :Wth = int(85)
'Dim Hht :Hht = int(100)

If OSversion < 520000 Then
  Wth = int(72)
  Hht = int(74)
  mainlogo.style.marginLeft = "7px"
  txt1.style.marginLeft = "0px"
Else
  Wth = int(70)
  Hht = int(72)
  mainlogo.style.marginLeft = "6px"
  txt1.style.marginLeft = "-1px"
End If

  window.ResizeTo Wth, Hht
  MoveTo ((Screen.Width / 2) - (Wth / 2)),((Screen.Height / 2) - (Hht / 2))

If OSversion > 610000 Then
If OSversion < 620000 Then
  mainlogo.style.marginLeft = "7px"
  txt1.style.marginLeft = "0px"
End If
End If

  Dim F1 :F1 = "<FONT STYLE='Font:20.0pt;Color:#000000;Font-Weight:Bold;'>"
  Dim strx, GetDecimalChar
  strx = CStr(CDbl(1/2))
  GetDecimalChar = Mid(strx, 2, 1)
  If GetDecimalChar = "" Then
  GetDecimalChar = ","
  End If
  Dim D1, D2, D3

   Function VB_Loader()
On Error Resume Next
InitC = "0"
arrCommands = Split(AppBootUp.commandLine, " /:")
   For i = 1 to (Ubound(arrCommands))
	InitC = arrCommands(1)
   Next
If InitC = "0" Then
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If
OSversion = VersionOS.value
If OSversion < 609999 Then
Set objFSO = CreateObject("Scripting.FileSystemObject")
If (objFSO.FileExists("Icon.png")) Then
mainlogo.innerHTML = "<img id=""mainlogoimg"" style=""width:48px;height:48px"" src=""Icon.png"">"
Else
mainlogo.innerHTML = ""
End If
Set objFSO = Nothing
Else
mainlogo.innerHTML = "<img id=""mainlogoimg"" style=""width:48px;height:48px"" src=""data:image/png;base64,"">"
End If
	txt1.innerHTML = F1 & "</FONT>"
	PointerA()
	Check()
	idTimer = window.setTimeout("MyTimer2", 30000, "VBScript")
   End Function
   Function PointerA()
On Error Resume Next
Pt = point.value
F2 = "<FONT style=""FONT-SIZE: 20pt; FONT-VARIANT: normal; FONT-WEIGHT: bold; COLOR: #000000; FONT-STYLE: normal; LINE-HEIGHT: normal"">"
Pt0 = F2 & "</FONT>"
Pt1 = F2 & ".</FONT>"
Pt2 = F2 & "..</FONT>"
Pt3 = F2 & "...</FONT>"
Pt4 = F2 & "....</FONT>"
Pit0 = "1"
Pit1 = "2"
Pit2 = "3"
Pit3 = "4"
Pit4 = "5"
If Pt = Pit1 Then
txt1.innerHTML = Pt2
point.value = Pit2
idTimer = window.setTimeout("PointerA", 500, "VBScript")
Exit Function
ElseIf Pt = Pit2 Then
txt1.innerHTML = Pt3
point.value = Pit3
idTimer = window.setTimeout("PointerA", 500, "VBScript")
Exit Function
ElseIf Pt = Pit3 Then
txt1.innerHTML = Pt4
point.value = Pit4
idTimer = window.setTimeout("PointerA", 500, "VBScript")
Exit Function
ElseIf Pt = Pit4 Then
txt1.innerHTML = Pt0
point.value = Pit0
idTimer = window.setTimeout("PointerA", 500, "VBScript")
Exit Function
ElseIf Pt = Pit0 Then
txt1.innerHTML = Pt1
point.value = Pit1
idTimer = window.setTimeout("PointerA", 500, "VBScript")
Exit Function
End If
   End Function
   Function Check()
On Error Resume Next
Set objFSO = CreateObject("Scripting.FileSystemObject")
If NOT (objFSO.FileExists("InitUpdate.vbs")) Then
	idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If
Set objFSO = Nothing
Set objFSO2 = CreateObject("Scripting.FileSystemObject")
Set objRead2 = objFSO2.OpenTextFile("Boot.log", 1, False)
D2 = objRead2.ReadLine
Set objFSO2 = Nothing
Set objRead2 = Nothing
If D2 = "stop" Then
	idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
Else
	idTimer = window.setTimeout("Check", 100, "VBScript")
End If
Exit Function
   End Function
   Function HtaExit()
	On Error Resume Next
	window.clearTimeout(idTimer)
	idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
   End Function
   Function MyTimer2()
	On Error Resume Next
	window.close()
	window.clearTimeout(idTimer)
   Exit Function
   End Function
</script>
