Exit Function
ElseIf Pt = Pit3 Then
pointB.value = Pit0
idTimer = window.setTimeout("PointerB", 500, "VBScript")
Exit Function
ElseIf Pt = Pit0 Then
pointB.value = Pit1
idTimer = window.setTimeout("PointerB", 500, "VBScript")
Exit Function
End If
   Exit Function
   End Function
   Function ImportImageDataB()
On Error Resume Next

OSversion = VersionOS.value
ModeUI = UIMode.value
If NOT ModeUI = "boot" Then
Exit Function
End If

Set objFSOI = CreateObject("Scripting.FileSystemObject")
Set objReadI = objFSOI.OpenTextFile("ImageData.tgib64", 1, False)
DImage = objReadI.ReadAll
Set objFSOI = Nothing
Set objReadI = Nothing
DImagec = Split(DImage, "-!.!-")

DImageData = "*$|$*"

For Each ImageD In DImagec

If OSversion < 520000 Then
serverlogo.title = "O Windows XP é imcompatível com imagens Base64!"
ErrorImage = "error.png"
DImageData = DImageData & ErrorImage &"*$|$*"
Else
If ImageD = "" Then
ErrorImage = "data:image/png;base64,"
DImageData = DImageData & ErrorImage &"*$|$*"
Else
ImageDr = Split(ImageD, "+-!-+")
   For i = 1 to (Ubound(ImageDr))
	ImageID = ImageDr(0)
	ImageD = ImageDr(1)
   Next

DImageData = DImageData & ImageD &"*$|$*"
End If
End If

Next

imagedatacacheB.value = DImageData
ServerCheckB()
   Exit Function
   End Function
Function ImageShowB( ByVal imageIDI )
