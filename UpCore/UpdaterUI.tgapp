Exit Function
End If

Pt = pointB.value
Pit0 = "   "
Pit1 = ".  "
Pit2 = ".. "
Pit3 = "..."
If Pt = Pit1 Then
pointB.value = Pit2
idTimer = window.setTimeout("PointerB", 500, "VBScript")
