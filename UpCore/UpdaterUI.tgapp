   End Function
   Function CloseTimeoutB(atime)
On Error Resume Next
OkB2.innerHTML = "Ok("&atime&")"
If atime = 0 Then
idTimer = window.setTimeout("MyTimer2", 500, "VBScript")
Else
atime = atime-1
idTimer = window.setTimeout("CloseTimeoutB("""&atime&""")", 1000, "VBScript")
End If
   Exit Function
   End Function
   Function HtaExit() 
On Error Resume Next
window.clearTimeout(idTimer)
Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("Status.log", 2, True)
objRead.WriteLine "cancelar"
Set objFSO = Nothing
Set objRead = Nothing
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
   End Function
   Function MyTimer2()
