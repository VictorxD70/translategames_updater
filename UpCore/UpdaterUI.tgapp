Set objWsh = CreateObject("WScript.Shell")
objWsh.Run "RoutineRestart.vbs /Init:Start /code:"& Code &" /upcore:"& UpCoreVersion &" /mode:UI", 0, 0

SEPageB.style.display = "none"

BootInit()
