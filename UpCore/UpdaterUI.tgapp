idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If
Set objFSO2 = CreateObject("Scripting.FileSystemObject")
Set objRead2 = objFSO2.OpenTextFile("StatusPS.log", 1, False)
D2 = objRead2.ReadLine
Set objFSO2 = Nothing
Set objRead2 = Nothing
If D2 = "close" Then
	idTimer = window.setTimeout("ProgressInit", 1, "VBScript")
	Exit Function
End If
If Mode = "update" Then
	txt1B.innerHTML = F1 & "Procurando Atualizações</FONT>"
ElseIf Mode = "install" Then
	txt1B.innerHTML = F1 & "Obtendo Informações</FONT>"
End If
	txt3B.innerHTML = F1 & "</FONT>"
	txt2B.innerHTML = F1 & "0%</FONT>"
	barB.style.width = "0%"
	CounterB()
	ImportImageDataB()
	PointerB()
	Progress.style.display = "none"
