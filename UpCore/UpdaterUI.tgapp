txt7B.innerHTML = F1 & "Tradução já atualizada!</FONT>"
txt8B.innerHTML = Replace(F1,"000000","008000") & "A versão mais recente já está instalada!</FONT>"
OkB2.style.display = "block"
txt9B.style.display = "block"
txtVB.style.display = "block"

Set oShell = CreateObject("WScript.Shell")

Version = "0"
If code = "350" Then
Version = oShell.RegRead("HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\TranslateGames(350-1)\DisplayVersion")
Else
Version = oShell.RegRead("HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\TranslateGames("& code &")\DisplayVersion")
End If
If code = "350" Then
InstallDate = oShell.RegRead("HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\TranslateGames(350-1)\InstallDate")
Else
InstallDate = oShell.RegRead("HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\TranslateGames("& code &")\InstallDate")
End If
If code = "350" Then
InstallTime = oShell.RegRead("HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\TranslateGames(350-1)\InstallTime")
Else
InstallTime = oShell.RegRead("HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\TranslateGames("& code &")\InstallTime")
End If
If (InstallDate) Then

dteCurrent = Date()
dteYesterday = Date() - 1
dteDay = Day(dteCurrent)
dteMonth = Month(dteCurrent)
dteYear = Year(dteCurrent)
dteDay2 = Day(dteYesterday)
dteMonth2 = Month(dteYesterday)
dteYear2 = Year(dteYesterday)

Ano = left(InstallDate,4)
InstallDate2 = mid(InstallDate,5)
Mes = left(InstallDate2,1)
Dia = mid(InstallDate2,2)
If Mes <= 1 Then
LInstallDate2 = len(InstallDate2)
If LInstallDate2 >= 3 Then
Mes = left(InstallDate2,2)
Dia = mid(InstallDate2,3)
ElseIf LInstallDate2 = 2 Then
Mes = left(InstallDate2,1)
Dia = mid(InstallDate2,2)
End If
End If
If len(Mes) = 1 OR Mes = 0 Then
Mes = "0"& CStr(Mes)
End If
If len(Dia) = 1 OR Dia = 0 Then
Dia = "0"& CStr(Dia)
End If
If len(dteMonth) = 1 OR dteMonth = 0 Then
dteMonth = "0"& CStr(dteMonth)
End If
If len(dteDay) = 1 OR dteDay = 0 Then
dteDay = "0"& CStr(dteDay)
End If
If len(dteMonth2) = 1 OR dteMonth2 = 0 Then
dteMonth2 = "0"& CStr(dteMonth2)
End If
If len(dteDay2) = 1 OR dteDay2 = 0 Then
dteDay2 = "0"& CStr(dteDay2)
End If
Actual = dteYear&dteMonth&dteDay
Registered = Ano&Mes&Dia
Yesterday = dteYear2&dteMonth2&dteDay2
If Registered = Actual Then
Result = "Hoje"
ElseIf Registered = Yesterday Then
Result = "Ontem"
Else
Result = Dia&"/"&Mes&"/"&Ano
End If
Data = "Última atualização: "&Result
Else
Data = ""
End If
If (InstallTime) Then

InstallTimec = Split(InstallTime,":")
   For i = 1 to (Ubound(InstallTimec))
	Hora = InstallTimec(0)
	Minuto = InstallTimec(1)
	Segundo = InstallTimec(2)
   Next

Result = "às "&Hora&":"&Minuto
Tempo = Result
Else
Tempo = ""
End If
If NOT Version = 0 Then
txtVB.innerHTML = Replace(F1,"000000","282828") &"Versão instalada: "&Version&"</FONT>"
End If
If NOT Data = "" Then
txt9B.innerHTML = Replace(F1,"000000","282828") & Data &" "& Tempo &"</FONT>"
End If
ElseIf msg = "connerror" Then
timeoutMSG = 0
ImageInfo = ImageShowB(4)
txt7B.innerHTML = F1 & "Falha ao conectar</FONT>"
txt8B.innerHTML = Replace(F1,"000000","8B0000") & "Ocorreu um erro ao tentar conectar com o servidor!</FONT>"

Set objFSOP = CreateObject("Scripting.FileSystemObject")
Set objReadP = objFSOP.OpenTextFile("UpCoreFCE.log", 1, False)
D1P = objReadP.ReadLine
Set objFSOP = Nothing
Set objReadP = Nothing
D1Pc = Split(D1P, "-")
   For i = 1 to (Ubound(D1Pc))
	UpCoreVersion = D1Pc(1)
   Next
UpCoreVersion = Replace(UpCoreVersion," ","")

Set objFSO = CreateObject("Scripting.FileSystemObject")
If NOT (objFSO.FileExists("RoutineRestart.vbs")) Then
RestartButtonsB.style.display = "none"
OkB2.style.display = "block"
Else
If Code = "0" Then
RestartButtonsB.style.display = "none"
OkB2.style.display = "block"
Else
If UpCoreVersion = "" Then
RestartButtonsB.style.display = "none"
OkB2.style.display = "block"
Else
OkB2.style.display = "none"
RestartButtonsB.style.display = "block"
End If
End If
End If
Set objFSO = Nothing

Else
Exit Function
End If

barB.style.backgroundcolor = "rgb(43,194,83)"
If OSversion < 609999 Then
InfoImageB.style.display = "none"
txt7TB.style.marginLeft = "5px"
txt8TB.style.marginLeft = "5px"
txt9TB.style.marginLeft = "5px"
txtVTB.style.marginLeft = "5px"
txt9B.style.fontSize = "9pt"
Else
If ImageInfo = "" Then
InfoImageB.style.display = "none"
txt7TB.style.marginLeft = "5px"
txt8TB.style.marginLeft = "5px"
txt9TB.style.marginLeft = "5px"
txtVTB.style.marginLeft = "5px"
txt9B.style.fontSize = "9pt"
ElseIf ImageInfo = "data:image/png;base64," Then
InfoImageB.style.display = "none"
txt7TB.style.marginLeft = "5px"
txt8TB.style.marginLeft = "5px"
txt9TB.style.marginLeft = "5px"
txtVTB.style.marginLeft = "5px"
txt9B.style.fontSize = "9pt"
Else
infoimgB.src = ImageInfo
InfoImageB.style.display = "block"
End If
End If

If timeoutMSG = 1 Then
idTimer = window.setTimeout("CloseTimeoutB(5)", 1, "VBScript")
Else
window.clearTimeout(idTimer)
End If

Boot.style.display = "none"
SEPageB.style.display = "block"
   Exit Function
   End Function
   Function RoutineRestartB()
On Error Resume Next

Code = "0"
arrCommands = Split(AppUpdaterUI.commandLine, " /:")
   For i = 1 to (Ubound(arrCommands))
	Code = arrCommands(3)
   Next

If Code = "0" Then
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If

Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("StatusIS.log", 2, True)
objRead.WriteLine "ready"
Set objFSO = Nothing
Set objRead = Nothing
