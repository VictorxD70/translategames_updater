Function CheckTotal()
Dim D2, File
On Error Resume Next

ModeUI = UIMode.value
If NOT ModeUI = "progress" Then
Exit Function
End If

File = filecache.value

Set objFSO = CreateObject("Scripting.FileSystemObject")
If NOT (objFSO.FileExists("UpdateLog.txt")) Then
totalsizecache.value = 0
idTimer = window.setTimeout("CheckTotal", 2000, "VBScript")
Exit Function
End If
Set objFSO = Nothing

Set objFSO2 = CreateObject("Scripting.FileSystemObject")
Set objRead2 = objFSO2.OpenTextFile("UpdateLog.txt", 1, False)
D2 = objRead2.ReadAll
Set objFSO2 = Nothing
Set objRead2 = Nothing

D2r = Split(D2, "Length: ")

D2data = ""

For Each D2s In D2r

D2fsr = Split(D2s, "Saving to: '")
   For i = 1 to (Ubound(D2fsr))
	D2ft = D2fsr(1)
   Next
D2ftr = Split(D2ft, "'")
   For i = 1 to (Ubound(D2ftr))
	Filet = D2ftr(0)
   Next

If Filet = File Then
D2sr = Split(D2s, " [")
   For i = 1 to (Ubound(D2sr))
	D2dataNR = D2sr(0)
   Next
D2sr = Split(D2dataNR, " (")
   For i = 1 to (Ubound(D2sr))
	D2dataNR = D2sr(0)
   Next
D2data = D2data &"|"& D2dataNR
End If

Next

D2dt = Split(D2data, "|")

For Each D2dtr In D2dt
D2dataR = D2dtr
Next

If D2dataR = "" Then
D2dataR = 0
End If

totalsizecache.value = D2dataR

idTimer = window.setTimeout("CheckTotal", 2000, "VBScript")
   Exit Function
   End Function
