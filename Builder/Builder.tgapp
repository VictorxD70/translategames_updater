<script language='vbscript'>
On Error Resume Next
Set objWMIService = GetObject("winmgmts:\\.\root\cimv2")
Set colOperatingSystems = objWMIService.ExecQuery("Select * from Win32_OperatingSystem")
For Each objOperatingSystem in colOperatingSystems
	OSversion = replace(objOperatingSystem.Version,".","")
Next
VersionOS.value = OSversion
UIMode.value = ""

  Dim strx, GetDecimalChar
  strx = CStr(CDbl(1/2))
  GetDecimalChar = Mid(strx, 2, 1)
  If GetDecimalChar = "" Then
  GetDecimalChar = ","
  End If

If OSversion < 520000 Then
  Wth = int(440)
  Hht = int(195)
  Hht2 = Hht - int(42)
  Wth2 = Wth - int(20)
Else
  Wth = int(430)
  Hht = int(190)
  Hht2 = Hht - int(39)
  Wth2 = Wth - int(20)
End If

If OSversion > 999999 Then
If h < 152 Then
  calc = 151 - h
  If calc = 0 Then
    multiplierW = 0
    multiplierH = 0
    multiplierH2 = 45
  Else
    multiplierW = calc*10
    multiplierH = calc*5
    multiplierH2 = 45+calc
  End If
  Wth = int(470) + multiplierW
  Hht = int(210) + multiplierH
  Hht2 = Hht - multiplierH2
  Wth2 = Wth - int(20)
End If
End If

  AllUI.style.height = Hht2&"px"
  AllUI.style.width = Wth2&"px"
  window.ResizeTo Wth, Hht
  MoveTo ((window.screen.availWidth/2)-(Wth/2)),((window.screen.availHeight/2)-(Hht/2)+20)

  Dim F1, F2 :F1 = "<FONT STYLE='Font:10.0pt;Color:#000000;Font-Weight:Bold;'>" :F2 = "<FONT STYLE='Font-Size:8.0pt'>"

   Function VB_Loader()
On Error Resume Next
InitC = "0"
arrCommands = Split(AppBuilderUp.commandLine, " /:")
   For i = 1 to (Ubound(arrCommands))
	InitC = arrCommands(1)
   Next
If InitC = "0" Then
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If
Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("VersionData\UpCore.log", 1, False)
UpCore = objRead.ReadLine
Set objRead = Nothing
Set objFSO = Nothing
UpCorec = Split(UpCore, "UpCore-->")
   For i = 1 to (Ubound(UpCorec))
	DataResult = UpCorec(1)
   Next
UpCored = Split(DataResult, "<--!")
   For i = 1 to (Ubound(UpCored))
	UpCoreRV = UpCored(0)
   Next
UpCoreRV = Replace(UpCoreRV," ","")
If UpCoreRV = "" Then
msgbox "Dados de Versão faltando!",vbCritical,"Erro"
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If
UpCoreT.innerHTML = "UpCore: "& UpCoreRV
Set objFSO = CreateObject("Scripting.FileSystemObject")
If NOT objFSO.FileExists("VersionData\Name.log") Then
msgbox "Dados de Versão faltando!",vbCritical,"Erro"
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If
Set objFSO = Nothing
BootInit()
   End Function
   Function BootInit()
On Error Resume Next
OSversion = VersionOS.value
UIMode.value = "boot"
Mode = "update"
InitC = "0"
Root = "0"
arrCommands = Split(AppBuilderUp.commandLine, " /:")
   For i = 1 to (Ubound(arrCommands))
	InitC = arrCommands(1)
	RootS = arrCommands(2)
   Next
If InitC = "0" Then
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
Exit Function
End If
RootS = Replace(RootS,"""","")
Root.value = RootS
Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("VersionData\Name.log", 1, False)
Name = objRead.ReadAll
Set objRead = Nothing
NameS = Split(Name, "<--!")
Set objRead2 = objFSO.OpenTextFile("LastCreated.log", 1, False)
Config = objRead2.ReadLine
Set objRead2 = Nothing
Set objFSO = Nothing
Configc = Split(Config, "|")
   For i = 1 to (Ubound(Configc))
	ConfigS = Configc(1)
   Next

UpSelect = "<select name=""Updater"" id=""Updater"">"

For Each NameE In NameS

If NameE = "" Then

Else

Namec = Split(NameE, "-->")
   For i = 1 to (Ubound(Namec))
	Code = Namec(0)
	TName = Namec(1)
   Next
Code = Replace(Code,vbCrLf,"")

If NOT Code = "" Then
If NOT TName = "" Then
If ConfigS = Code Then
UpSelect = UpSelect&"<option value="""&Code&""" selected=""selected"">Tradução de "&TName&"</option>"
Else
UpSelect = UpSelect&"<option value="""&Code&""">Tradução de "&TName&"</option>"
End If
Else
If ConfigS = Code Then
UpSelect = UpSelect&"<option value="""&Code&""" selected=""selected"">Tradução Código: "&Code&"</option>"
Else
UpSelect = UpSelect&"<option value="""&Code&""">Tradução Código: "&Code&"</option>"
End If
End If
End If

End If

Next

UpSel.innerHTML = UpSelect&"</select>"

document.title = "Criador do Atualizador"

If UIMode.value = "boot" Then
Progress.style.display = "none"
Boot.style.display = "block"
End If

   End Function
   Function StartBuild()
On Error Resume Next
Set objFSO = CreateObject("Scripting.FileSystemObject")
If NOT objFSO.FileExists("StartBuild.vbs") Then
msgbox "Arquivo necessário: ""Builder\StartBuild.vbs"" não encontrado!",vbCritical,"Faltando Arquivo!"
Exit Function
End If
Set objFSO = Nothing
Boot.style.display = "none"
OSversion = VersionOS.value
RootS = Root.value
UpdaterS = Updater.value
FUblkS = FUblk.value
Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("LastCreated.log", 2, True)
	objRead.WriteLine("|"&UpdaterS&"|")
Set objRead = Nothing
Set objFSO = Nothing
If OSversion < 520000 Then
PbarN.style.width = "415px"
PbarBTN.style.width = "415px"
loaderN.style.width = "415px"
bar.style.maxWidth = "415px"
End If
Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("VersionData\Name.log", 1, False)
Name = objRead.ReadAll
Set objRead = Nothing
Set objFSO = Nothing
NameS = Split(Name, "<--!")
For Each NameE In NameS
If NameE = "" Then

Else
Namec = Split(NameE, "-->")
   For i = 1 to (Ubound(Namec))
	Code = Namec(0)
	TName = Namec(1)
   Next
Code = Replace(Code,vbCrLf,"")
If NOT Code = "" Then
If NOT TName = "" Then
If UpdaterS = Code Then
NameST = TName
End If
Else
If UpdaterS = Code Then
NameST = Code
End If
End If
End If
End If
Next
bar.style.width = "0%"
document.title = "Criando Atualizador da Tradução de "&NameST&"..."
txt1.innerHTML = F1 & "Preparando...</FONT>"
UIMode.value = "progress"

	idTimer = window.setTimeout("Counter", 1, "VBScript")
	idTimer = window.setTimeout("PointerP", 1, "VBScript")
	idTimer = window.setTimeout("ProgressBarAnim(""0"")", 1, "VBScript")

Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("Status.log", 2, True)
objRead.WriteLine("|Creating|Preparando|")
Set objRead = Nothing
Set objFSO = Nothing

Progress.style.display = "block"

Set objWsh = CreateObject("WScript.Shell")
If FUblkS = "no" Then
objWsh.Run "powershell -command ""Unblock-File -Path "&RootS&"\Builder\StartBuild.vbs""", 0, 1
FUblk.value = "yes"
End If
objWsh.Run "StartBuild.vbs /code:"& UpdaterS &" /root:"""& RootS &""" /FUblk:"&FUblkS&"", 0, 0
   End Function
   Function PointerP()
On Error Resume Next
FunctionClock = 600
ModeUI = UIMode.value
If NOT ModeUI = "progress" Then
Exit Function
End If
Pt = point.value
Pit0 = ""
Pit1 = "."
Pit2 = ".."
Pit3 = "..."
If Pt = Pit1 Then
point.value = Pit2
idTimer = window.setTimeout("PointerP", FunctionClock, "VBScript")
Exit Function
ElseIf Pt = Pit2 Then
point.value = Pit3
idTimer = window.setTimeout("PointerP", FunctionClock, "VBScript")
Exit Function
ElseIf Pt = Pit3 Then
point.value = Pit0
idTimer = window.setTimeout("PointerP", FunctionClock, "VBScript")
Exit Function
ElseIf Pt = Pit0 Then
point.value = Pit1
idTimer = window.setTimeout("PointerP", FunctionClock, "VBScript")
Exit Function
End If
   Exit Function
   End Function
   Function ProgressBarAnim(AnimN)
On Error Resume Next
FunctionClock = 16

ModeUI = UIMode.value
If NOT ModeUI = "progress" Then
bar.style.marginLeft = "0px"
Exit Function
End If

PGC = PGCache.value
PGCc = Split(PGC, ".")
   For i = 1 to (Ubound(PGCc))
	PGC = PGCc(0)
   Next
If PGC > 0 Then
PGC = PGC + 1
End If

ModeAnim = PGAnim.value
If NOT ModeAnim = "yes" Then
bar.style.marginLeft = "0px"
idTimer = window.setTimeout("ProgressBarAnim("""&PGC&""")", 65, "VBScript")
Exit Function
End If

If AnimN < 6 Then
bar.style.width = "5%"
calc = 5-AnimN
bar.style.marginLeft = "-"&calc&"%"
ElseIf AnimN > 5 AND AnimN < 51 Then
bar.style.width = AnimN&"%"
bar.style.marginLeft = "0px"
ElseIf AnimN > 50 AND AnimN < 101 Then
bar.style.width = "50%"
calc = AnimN-50
bar.style.marginLeft = calc&"%"
ElseIf AnimN > 100 AND AnimN < 146 Then
calc = 150-AnimN
bar.style.width = calc&"%"
calc = AnimN-50
bar.style.marginLeft = calc&"%"
ElseIf AnimN > 145 AND AnimN < 151 Then
bar.style.width = "5%"
calc = AnimN-50
bar.style.marginLeft = calc&"%"
End If

If AnimN = 150 OR AnimN > 150 Then
idTimer = window.setTimeout("ProgressBarAnim(""0"")", FunctionClock, "VBScript")
Else
AnimN = AnimN + 1
idTimer = window.setTimeout("ProgressBarAnim("""&AnimN&""")", FunctionClock, "VBScript")
End If

   Exit Function
   End Function
   Function SetProgressBar(PGP)
On Error Resume Next

PGCache.value = PGP
ModeAnim = PGAnim.value
If PGP = 0 Then
PGAnim.value = "yes"
Else
PGAnim.value = "no"
If ModeAnim = "yes" Then
bar.style.width = "0px"
bar.style.marginLeft = "0px"
End If
bar.style.width = PGP&"%"
End If

   Exit Function
   End Function
   Function Counter()
On Error Resume Next
FunctionClock = 65

ModeUI = UIMode.value
If NOT ModeUI = "progress" Then
Exit Function
End If

Pt = point.value

Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("Status.log", 1, False)
D2 = objRead.ReadLine
Set objFSO = Nothing
Set objRead = Nothing
D2c = Split(D2, "|")
   For i = 1 to (Ubound(D2c))
	Status = D2c(1)
	Msg = D2c(2)
   Next

If (Status) Then
LastStatus.value = Status
If Status = "Created" Then
Temp = SetProgressBar(100)
idTimer = window.setTimeout("FinishBuild", 1, "VBScript")
Exit Function
End If
End If

If (Msg) Then
txt1.innerHTML = F1 & Msg & Pt &"</FONT>"
Else
txt1.innerHTML = F1 & "Criando Atualizador"&Pt&"</FONT>"
End If

idTimer = window.setTimeout("Counter", FunctionClock, "VBScript")
Exit Function
   End Function
   Function FinishBuild()
On Error Resume Next
UIMode.value = "finish"
OSversion = VersionOS.value
RootS = Root.value
UpdaterS = Updater.value
Set objFSO = CreateObject("Scripting.FileSystemObject")
objFSO.DeleteFile("Status.log")
Set objFSO = Nothing
Set objFSO = CreateObject("Scripting.FileSystemObject")
Set objRead = objFSO.OpenTextFile("VersionData\Name.log", 1, False)
Name = objRead.ReadAll
Set objRead = Nothing
Set objFSO = Nothing
NameS = Split(Name, "<--!")
For Each NameE In NameS
If NameE = "" Then

Else
Namec = Split(NameE, "-->")
   For i = 1 to (Ubound(Namec))
	Code = Namec(0)
	TName = Namec(1)
   Next
Code = Replace(Code,vbCrLf,"")
If NOT Code = "" Then
If NOT TName = "" Then
If UpdaterS = Code Then
NameST = TName
End If
Else
If UpdaterS = Code Then
NameST = Code
End If
End If
End If
End If
Next
txt1.innerHTML = F1 & "</FONT>"
document.title = "Atualizador Criado!"
RestartButtons.style.display = "block"
txt7.innerHTML = F1 & "Atualizador Criado!</FONT>"
txt8.innerHTML = Replace(F1,"000000","008000") & "Atualizador da Tradução de "&NameST&" criado!</FONT>"
ProgressUI.style.display = "none"
SEPage.style.display = "block"
   Exit Function
   End Function
   Function ShowFolder()
On Error Resume Next
RootS = Root.value
UpdaterS = Updater.value

Set objWsh = CreateObject("WScript.Shell")
objWsh.Run """"& RootS &"\Build\"& UpdaterS &"\""", 1, 0

   Exit Function
   End Function
   Function RoutineRestart()
On Error Resume Next

Temp = SetProgressBar(0)
LastStatus.value = ""
SEPage.style.display = "none"
Progress.style.display = "none"
ProgressUI.style.display = "block"

BootInit()
   Exit Function
   End Function
   Function HtaExit() 
On Error Resume Next
window.clearTimeout(idTimer)
idTimer = window.setTimeout("MyTimer2", 1, "VBScript")
   End Function
   Function MyTimer2()
On Error Resume Next
window.close()
window.clearTimeout(idTimer)
   Exit Function
   End Function
</script>
